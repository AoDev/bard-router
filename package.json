{
  "name": "bard-router",
  "version": "2.0.2",
  "description": "More than just a routing solution, for React/Mobx applications",
  "homepage": "https://github.com/AoDev/bard-router",
  "main": "lib/index.js",
  "scripts": {
    "build:js": "del-cli lib && babel src --out-dir lib --extensions '.ts,.tsx,.js' --ignore '**/*.spec.js'",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "npm run build:js && npm run build:types",
    "lint": "eslint src --quiet",
    "prepare": "NODE_ENV=production && npm run build",
    "tag-release": "npm test && standard-version",
    "test-unit": "jest",
    "test": "jest"
  },
  "keywords": [
    "router",
    "react",
    "mobx"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:AoDev/bard-router.git"
  },
  "author": "Kevin Purnelle <kevin.purnelle@gmail.com> (http://kevinpurnelle.com/)",
  "license": "MIT",
  "dependencies": {
    "history": "^4.10.1"
  },
  "devDependencies": {
    "@babel/cli": "7.21.0",
    "@babel/compat-data": "7.21.0",
    "@babel/core": "7.21.0",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "7.20.7",
    "@babel/plugin-transform-runtime": "7.21.0",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.0",
    "@babel/runtime": "7.21.0",
    "@types/history": "4.7.8",
    "@types/jest": "29.4.0",
    "@types/react": "17.0.13",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "29.4.3",
    "babel-plugin-add-module-exports": "1.0.4",
    "babel-plugin-module-resolver": "5.0.0",
    "del-cli": "3.0.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.7",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "jest": "29.4.3",
    "mobx": "6.8.0",
    "mobx-react": "7.6.0",
    "prettier": "2.8.4",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "standard-version": "9.5.0",
    "typescript": "4.9.5"
  }
}
